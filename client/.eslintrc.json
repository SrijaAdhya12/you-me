{
	"extends": ["next/core-web-vitals", "next/typescript", "prettier"],
	"rules": {
		"react/no-unescaped-entities": "off",
		"@next/next/no-page-custom-font": "off",
		"@typescript-eslint/no-unused-vars": [
			"warn",
			{
				"vars": "all",
				"varsIgnorePattern": "^_",
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		],
		"@typescript-eslint/no-unused-expressions": "off"
	}
}

